<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="eth.bruises.pet.mapper.PetOnlineAuditLogMapper">
    <!--保存对象-->
    <insert id="add" parameterType="PetOnlineAuditLog" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        insert into t_pet_online_audit_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
                                            <if test="petId != null">
                            pet_id,
                        </if>
                                            <if test="state != null">
                            state,
                        </if>
                                            <if test="auditId != null">
                            audit_id,
                        </if>
                                            <if test="auditTime != null">
                            audit_time,
                        </if>
                                            <if test="note != null">
                            note,
                        </if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">
                                            <if test="petId != null">
#{petId}                            ,
                        </if>
                                            <if test="state != null">
#{state}                            ,
                        </if>
                                            <if test="auditId != null">
#{auditId}                            ,
                        </if>
                                            <if test="auditTime != null">
#{auditTime}                            ,
                        </if>
                                            <if test="note != null">
#{note}                            ,
                        </if>
        </trim>
    </insert>
    <!--删除对象-->
    <delete id="delete" parameterType="long">
        delete from t_pet_online_audit_log where id = #{id}
    </delete>

    <!--更新对象-->
    <update id="update" parameterType="PetOnlineAuditLog">
        update t_pet_online_audit_log
        <set>
                                            <if test="petId != null">
                            pet_id = #{petId},
                        </if>
                                            <if test="state != null">
                            state = #{state},
                        </if>
                                            <if test="auditId != null">
                            audit_id = #{auditId},
                        </if>
                                            <if test="auditTime != null">
                            audit_time = #{auditTime},
                        </if>
                                            <if test="note != null">
                            note = #{note}                        </if>
        </set>
        where id = #{id}
    </update>
    <!--查找单个对象-->
    <select id="selectOne" resultMap="BaseResultMap" parameterType="long">
        select * from t_pet_online_audit_log where id = #{id}
    </select>
    <!--查找所有对象-->
    <select id="selectAll" resultMap="BaseResultMap">
        select * from t_pet_online_audit_log
    </select>
    <!--查询总数=参数（查询对象），返回值：long-->
    <select id="queryByCount" parameterType="PetOnlineAuditLogQuery" resultType="integer">
        select count(*) from t_pet_online_audit_log
        <include refid="whereSql"/>
    </select>
    <!--查询当前页数据=参数（查询对象），返回值:List-->
    <select id="queryByPage" parameterType="PetOnlineAuditLogQuery" resultMap="BaseResultMap">
        select * from t_pet_online_audit_log
        <include refid="whereSql"/>
        limit #{begin},#{pageSize}
    </select>


    <sql id="whereSql">
        <where>
            <if test="keyword!=null and keyword!=''">
                and (name like concat('%',#{keyword},'%'))
            </if>
        </where>
    </sql>

    <!--批量删除-->
    <delete id="batchDel" parameterType="arraylist">
        delete from t_pet_online_audit_log where id in
        <foreach collection="list" item="id" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </delete>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="eth.bruises.pet.domain.PetOnlineAuditLog">
        <id column="id" property="id" />
        <result column="pet_id" property="petId" />
        <result column="state" property="state" />
        <result column="audit_id" property="auditId" />
        <result column="audit_time" property="auditTime" />
        <result column="note" property="note" />
    </resultMap>

</mapper>
